{
  "name": "file-agents",
  "version": "1.0.0",
  "description": "Agents for reading and writing files in web and nodeJS",
  "type": "module",
  "main": "cjs/index.js",
  "module": "mjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs",
    "mjs"
  ],
  "scripts": {
    "tsc": "tsc",
    "build:commonjs": "rm -fr cjs/* && tsc -p tsconfig-cjs.json",
    "build:test": "rm -fr test/* && tsc -p tsconfig-test.json",
    "build:mjs": "rm -fr mjs/* && tsc -p tsconfig.json && bash mjs-formatter.sh",
    "build": "npm run build:commonjs && npm run build:mjs && ./fixup.sh",
    "clean": "npm prune --production",
    "test": "npm run build:test && jest ./test --coverage",
    "test:server": "npm run build:mjs && node ./assets/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xutyxd/file-agents.git"
  },
  "keywords": [
    "typescript",
    "agent",
    "read",
    "write",
    "nodejs",
    "web"
  ],
  "author": "XutyXD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xutyxd/file-agents/issues"
  },
  "homepage": "https://github.com/xutyxd/file-agents#readme",
  "devDependencies": {
    "@semantic-release/github": "^9.0.6",
    "@semantic-release/gitlab": "^9.2.0",
    "@semantic-release/npm": "^11.0.0",
    "@types/jest": "^29.5.3",
    "express": "^4.18.2",
    "jest": "^29.6.2",
    "semantic-release": "^22.0.1",
    "typescript": "^5.2.2"
  }
}
